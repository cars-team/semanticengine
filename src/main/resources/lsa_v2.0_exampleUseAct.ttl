@prefix : <http://www.gauss.it/lsa/> .
@prefix lsa: <http://www.gauss.it/lsa/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gmus: <http://example.museum.gauss.it/> .
@prefix owlc: <http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@base <http://www.gauss.it/lsa/> .

<http://www.gauss.it/lsa/> rdf:type owl:Ontology ;
                            owl:versionIRI <http://www.gauss.it/lsa/> ;
                            owl:imports sosa: ;
                            rdfs:comment "LSA Ontology v.2.0, its example use, its Alignment with SSN 2017 and OWLC."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
<http://purl.org/dc/terms/created> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/rights
<http://purl.org/dc/terms/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://schema.org/domainIncludes
<http://schema.org/domainIncludes> rdf:type owl:AnnotationProperty .


###  http://schema.org/rangeIncludes
<http://schema.org/rangeIncludes> rdf:type owl:AnnotationProperty .


###  http://www.gauss.it/lsa/hasControlSpecification
lsa:hasControlSpecification rdf:type owl:AnnotationProperty ;
                            rdfs:range lsa:ControlStructure .


###  http://www.gauss.it/lsa/hasMethod
lsa:hasMethod rdf:type owl:AnnotationProperty .


###  http://www.gauss.it/lsa/hasPriority
lsa:hasPriority rdf:type owl:AnnotationProperty ;
                rdfs:range xsd:int ,
                           xsd:integer ;
                rdfs:domain ssn:System .


###  http://www.gauss.it/lsa/implements
lsa:implements rdf:type owl:AnnotationProperty .


###  http://www.gauss.it/lsa/resr
lsa:resr rdf:type owl:AnnotationProperty .


###  http://www.opengis.net/ont/geosparql#asWKT
<http://www.opengis.net/ont/geosparql#asWKT> rdf:type owl:AnnotationProperty .


###  http://www.opengis.net/ont/geosparql#hasGeometry
<http://www.opengis.net/ont/geosparql#hasGeometry> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#comment
rdf:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
<http://www.w3.org/2004/02/skos/core#note> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/sosa/observes
sosa:observes rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:AnnotationProperty .


###  https://w3id.org/pep/madedBy
<https://w3id.org/pep/madedBy> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.opengis.net/ont/geosparql#wktLiteral
<http://www.opengis.net/ont/geosparql#wktLiteral> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.gauss.it/lsa/consists
lsa:consists rdf:type owl:ObjectProperty ;
             rdfs:domain lsa:State ;
             rdfs:range lsa:State .


###  http://www.gauss.it/lsa/executedBy
lsa:executedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf lsa:executes ;
               rdfs:domain lsa:SoftwareProcedure ;
               rdfs:range lsa:SoftwareProcedureExecutor ;
               rdfs:comment """lsa:executedBy

Scope note: This property identifies procedure executors of software procedures.

Range:          lsa:SoftwareProcedure

Domain:        lsa:ProcedureExecutor"""@en .


###  http://www.gauss.it/lsa/executes
lsa:executes rdf:type owl:ObjectProperty ;
             rdfs:domain lsa:SoftwareProcedureExecutor ;
             rdfs:range lsa:SoftwareProcedure ;
             rdfs:comment """lsa:executes

Scope note: This property identifies software procedures executed by procedure executors .

Domain:        lsa:ProcedureExecutor

Range:          lsa:SoftwareProcedure"""@en .


###  http://www.gauss.it/lsa/first
lsa:first rdf:type owl:ObjectProperty ;
          rdfs:domain lsa:List ;
          rdfs:comment """lsa:first

Scope note: This property identifies the first item in the list.

Domain:        lsa:List

Range:          owl:Thing"""@en .


###  http://www.gauss.it/lsa/hasActionSpecification
lsa:hasActionSpecification rdf:type owl:ObjectProperty ;
                           owl:inverseOf lsa:isActionSpecificationOf ;
                           rdfs:domain lsa:ActionableBehavior ;
                           rdfs:range lsa:Actionable ;
                           rdfs:comment """lsa:hasActionSpecification

Scope note: This property identifies the actionable items of the behavior for a software procedure.

Inverse:        lsa:isActionSpecificationOf

Domain:        lsa:ActionableBehavior

Range:          lsa:Actionable"""@en .


###  http://www.gauss.it/lsa/hasBehavior
lsa:hasBehavior rdf:type owl:ObjectProperty ;
                owl:inverseOf lsa:isBehaviorOf ;
                rdfs:domain lsa:SoftwareProcedure ;
                rdfs:range lsa:ActionableBehavior ;
                rdfs:comment """lsa:hasBehavior

Scope note: This property identifies the actionable behavior for a software procedure.

Domain:        lsa:SoftwareProcedure

Range:          lsa:ActionableBehaior"""@en .


###  http://www.gauss.it/lsa/hasControlSpecification
lsa:hasControlSpecification rdf:type owl:ObjectProperty ;
                            owl:inverseOf lsa:isControlSpecificationOf ;
                            rdfs:domain lsa:ActionableBehavior .


###  http://www.gauss.it/lsa/hasExecuted
lsa:hasExecuted rdf:type owl:ObjectProperty ;
                owl:inverseOf lsa:madeBy ;
                rdfs:domain lsa:SoftwareProcedureExecutor ;
                rdfs:range lsa:SoftwareProcedureExecution ;
                rdfs:comment """lsa:hasExecuted

Scope note: This property identifies software procedure executions executed by software procedure executors .

Domain:        lsa:ProcedureExecutor

Range:          lsa:SoftwareProcedure"""@en .


###  http://www.gauss.it/lsa/hasPreference
lsa:hasPreference rdf:type owl:ObjectProperty ;
                  rdfs:domain owlc:ContextDimension ;
                  rdfs:range owlc:ContextDimensionValue .


###  http://www.gauss.it/lsa/hasState
lsa:hasState rdf:type owl:ObjectProperty ;
             owl:inverseOf lsa:isStateOf ;
             rdfs:domain ssn:System ;
             rdfs:range lsa:State ;
             rdfs:comment """lsa:hasState

Scope note: This property specifies the states of a ssn:System.

inverse:         lsa:isStateOf

Domain:        ssn:System

Range:           lsa:State"""@en .


###  http://www.gauss.it/lsa/hasTimespan
lsa:hasTimespan rdf:type owl:ObjectProperty ;
                owl:inverseOf lsa:isTimespanOf ;
                rdfs:domain lsa:TemporalEntity ;
                rdfs:range lsa:Timespan ;
                rdfs:comment """lsa:hasTimespan 

Scope note: This property describes the temporal confinement of an instance of a Temporal Entity.

inverse:           lsa:isTimespanOf

Domain:         lsa:TemporalEntity
Range:           lsa:Timespan

Quantification: many to one, necessary, dependent (1,1:1,n)"""@en .


###  http://www.gauss.it/lsa/hasType
lsa:hasType rdf:type owl:ObjectProperty ;
            owl:inverseOf lsa:isTypeOf ;
            rdf:type owl:FunctionalProperty ;
            rdfs:range lsa:Type ;
            rdfs:comment """lsa:hasType

Scope note: This property allows typing of owl:Thing through terminalogical classifications of terminological hierarchy or thesaurus.

Inverse:       lsa:isTypeOf

Domain:      owl:Thing

Range:         lsa:Type"""@en .


###  http://www.gauss.it/lsa/implementedBy
lsa:implementedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf ssn:implementedBy ;
                  rdfs:domain lsa:SoftwareProcedure ;
                  rdfs:range ssn:System ;
                  rdfs:comment """lsa:implementedBy

Subproperty of: ssn:implementedBy

Scope note: This property identifies  systems that implement a software procedure.

Domain:       lsa:SoftwareProcedure

Range:         ssn:System"""@en .


###  http://www.gauss.it/lsa/isAbleToExecute
lsa:isAbleToExecute rdf:type owl:ObjectProperty ;
                    owl:inverseOf lsa:isExecutableBy ;
                    rdfs:domain lsa:SoftwareProcedureExecutor ;
                    rdfs:range lsa:ActionableType ;
                    rdfs:comment """lsa:isAbleToExecute

Scope note: This property specifies the actionable types executable by a software procedure executor.

inverse: lsa:isExecutableBy

Domain:        lsa:SoftwareProcedureExecutor

Range:          lsa:ActionableType"""@en .


###  http://www.gauss.it/lsa/isActionSpecificationOf
lsa:isActionSpecificationOf rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain lsa:Actionable ;
                            rdfs:range lsa:ActionableBehavior .


###  http://www.gauss.it/lsa/isBehaviorOf
lsa:isBehaviorOf rdf:type owl:ObjectProperty ;
                 rdfs:domain lsa:ActionableBehavior ;
                 rdfs:range lsa:SoftwareProcedure ;
                 rdfs:comment """lsa:isBehaviorOf

inverse:         lsa:hasBehavior

Scope note: This property identifies the  software procedures of the actionable behaviors.

Domain:       lsa:ActionableBehaior  

Range:          lsa:SoftwareProcedure"""@en .


###  http://www.gauss.it/lsa/isControlSpecificationOf
lsa:isControlSpecificationOf rdf:type owl:ObjectProperty ;
                             rdfs:domain lsa:ControlStructure ;
                             rdfs:range lsa:ActionableBehavior ;
                             rdfs:comment """lsa:isControlSpecificationOf

Scope note:  This property identifies the control structure of behaviors.

inverse:          lsa:hasControlSpecificationOf

Domain:         lsa:ControlStructure

Range:            lsa:ActionableBehaior"""@en .


###  http://www.gauss.it/lsa/isExecutableBy
lsa:isExecutableBy rdf:type owl:ObjectProperty ;
                   rdfs:domain lsa:ActionableType ;
                   rdfs:range lsa:SoftwareProcedureExecutor ;
                   rdfs:comment """lsa:isExecutableBy

Scope note: This property specifies the software procedure executors able to execute the actionable types.

inverse:        lsa:isAbleToExecute

Domain:       lsa:ActionableType

Range:          lsa:SoftwareProcedureExecutor"""@en .


###  http://www.gauss.it/lsa/isStateOf
lsa:isStateOf rdf:type owl:ObjectProperty ;
              rdfs:domain lsa:State ;
              rdfs:range ssn:System ;
              rdfs:comment """lsa:isStateOf

inverse:        lsa:hasState

Scope note: This property specifies the ssn:System characterized by the states .

Domain:        ssn:System

Range:           lsa:State"""@en .


###  http://www.gauss.it/lsa/isTimespanOf
lsa:isTimespanOf rdf:type owl:ObjectProperty ;
                 rdfs:domain lsa:Timespan ;
                 rdfs:range lsa:TemporalEntity ;
                 rdfs:comment """lsa:isTimespanOf 

Scope note: This property identifies the temporal entity of the timespan.

inverse:           lsa:hasTimespan

Domain:          lsa:Timespan
Range:            lsa:TemporalEntity 

Quantification: many to one, necessary, dependent (1,1:1,n)"""@en .


###  http://www.gauss.it/lsa/isTypeOf
lsa:isTypeOf rdf:type owl:ObjectProperty ;
             rdfs:domain lsa:Type ;
             rdfs:comment """lsa:isTypeOf 

Scope note: This property allows typing of owl:Thing through terminalogical classifications of terminological hierarchy or thesaurus.

Inverse:        lsa:hasType

Domain:       lsa:Type 

Range:         owl:Thing"""@en .


###  http://www.gauss.it/lsa/madeBy
lsa:madeBy rdf:type owl:ObjectProperty ;
           rdfs:domain lsa:SoftwareProcedureExecution ;
           rdfs:range lsa:SoftwareProcedureExecutor ;
           rdfs:comment """lsa:madeBy

Scope note: This property identifies one or more procedure executors for a procedure execution.

Inverse of:     lsa:hasExecuted

Domain:        lsa:ProcedureExecution

Range:          lsa:ProcedureExecutor"""@en .


###  http://www.gauss.it/lsa/rest
lsa:rest rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain lsa:List ;
         rdfs:comment """lsa:rest

Scope note: This property identifies the rest of the after lsa:first.

Domain:        lsa:List

Range:          owl:Thing"""@en .


###  http://www.gauss.it/lsa/usedProcedure
lsa:usedProcedure rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sosa:usedProcedure ;
                  rdfs:domain lsa:SoftwareProcedureExecution ;
                  rdfs:range lsa:SoftwareProcedure ;
                  rdfs:comment """lsa:usedProcedure

Subproperty of: sosa:usedProcedure

Scope note: This property identifies procedure executions for a procedure.

Domain:        lsa:ProcedureExecution

Range:          lsa:Procedure"""@en .


###  http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#constraintRefersTo
owlc:constraintRefersTo rdf:type owl:ObjectProperty ;
                        rdfs:domain owlc:ContextDimensionConstraint ;
                        rdfs:range owlc:ContextDimension .


###  http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#hasConstraint
owlc:hasConstraint rdf:type owl:ObjectProperty ;
                   rdfs:domain owlc:ContextDimension ;
                   rdfs:range owlc:ContextDimensionConstraint .


###  http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#hasDimension
owlc:hasDimension rdf:type owl:ObjectProperty ;
                  rdfs:domain owlc:Context ;
                  rdfs:range owlc:ContextDimension .


###  http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#hasObjectValue
owlc:hasObjectValue rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain owlc:ContextDimension ;
                    rdfs:range owlc:ContextDimensionValue .


###  http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#hasSubdimension
owlc:hasSubdimension rdf:type owl:ObjectProperty ;
                     owl:inverseOf owlc:isSubdimensionOf ;
                     rdfs:domain owlc:CompositeContextDimension ;
                     rdfs:range owlc:ContextDimension .


###  http://www.ing.unisannio.it/ding/semantics/OWL-C.owl#isSubdimensionOf
owlc:isSubdimensionOf rdf:type owl:ObjectProperty ;
                      rdfs:domain owlc:ContextDimension ;
                      rdfs:range owlc:CompositeContextDimension .


###  http://www.w3.org/ns/sosa/actsOnProperty
sosa:actsOnProperty rdf:type owl:ObjectProperty ;
                    owl:inverseOf sosa:isActedOnBy ;
                    <http://schema.org/domainIncludes> sosa:Actuation ;
                    <http://schema.org/rangeIncludes> sosa:ActuatableProperty ;
                    rdfs:comment "Relation between an Actuation and the property of a FeatureOfInterest it is acting upon."@en ;
                    rdfs:isDefinedBy sosa: ;
                    rdfs:label "acts on property"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Relation between an Actuation and the property of a FeatureOfInterest it is acting upon."@en ;
                    <http://www.w3.org/2004/02/skos/core#example> "In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property on which the Actuator acts upon is the state of the window as it changes from being open to being closed. "@en .


###  http://www.w3.org/ns/sosa/hasFeatureOfInterest
sosa:hasFeatureOfInterest rdf:type owl:ObjectProperty ;
                          owl:inverseOf sosa:isFeatureOfInterestOf ;
                          <http://schema.org/domainIncludes> sosa:Actuation ,
                                                             sosa:Observation ,
                                                             sosa:Sampling ;
                          <http://schema.org/rangeIncludes> sosa:FeatureOfInterest ,
                                                            sosa:Sample ;
                          rdfs:comment "A relation between an Observation and the entity whose quality was observed, or between an Actuation and the entity whose property was modified, or between an act of Sampling and the entity that was sampled."@en ;
                          rdfs:isDefinedBy sosa: ;
                          rdfs:label "has feature of interest"@en ;
                          <http://www.w3.org/2004/02/skos/core#definition> "A relation between an Observation and the entity whose quality was observed, or between an Actuation and the entity whose property was modified, or between an act of Sampling and the entity that was sampled."@en ;
                          <http://www.w3.org/2004/02/skos/core#example> "For example, in an Observation of the weight of a person, the FeatureOfInterest is the person and the property is its weight."@en .


###  http://www.w3.org/ns/sosa/hasResult
sosa:hasResult rdf:type owl:ObjectProperty ;
               owl:inverseOf sosa:isResultOf ;
               <http://schema.org/domainIncludes> sosa:Actuation ,
                                                  sosa:Observation ,
                                                  sosa:Sampling ;
               <http://schema.org/rangeIncludes> sosa:Result ,
                                                 sosa:Sample ;
               rdfs:comment "Relation linking an Observation or Actuation or act of Sampling and a Result or Sample."@en ;
               rdfs:isDefinedBy sosa: ;
               rdfs:label "has result"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relation linking an Observation or Actuation or act of Sampling and a Result or Sample."@en .


###  http://www.w3.org/ns/sosa/hasSample
sosa:hasSample rdf:type owl:ObjectProperty ;
               owl:inverseOf sosa:isSampleOf ;
               rdf:type owl:InverseFunctionalProperty ;
               <http://schema.org/domainIncludes> sosa:FeatureOfInterest ;
               <http://schema.org/rangeIncludes> sosa:Sample ;
               rdfs:comment "Relation between a FeatureOfInterest and the Sample used to represent it."@en ;
               rdfs:isDefinedBy sosa: ;
               rdfs:label "has sample"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relation between a FeatureOfInterest and the Sample used to represent it."@en .


###  http://www.w3.org/ns/sosa/hosts
sosa:hosts rdf:type owl:ObjectProperty ;
           owl:inverseOf sosa:isHostedBy ;
           owl:propertyChainAxiom ( ssn:inDeployment
                                    ssn:deployedSystem
                                  ) ;
           <http://schema.org/domainIncludes> sosa:Platform ;
           <http://schema.org/rangeIncludes> sosa:Actuator ,
                                             sosa:Platform ,
                                             sosa:Sampler ,
                                             sosa:Sensor ;
           rdfs:comment "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
           rdfs:isDefinedBy sosa: ;
           rdfs:label "hosts"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en .


###  http://www.w3.org/ns/sosa/isActedOnBy
sosa:isActedOnBy rdf:type owl:ObjectProperty ;
                 <http://schema.org/domainIncludes> sosa:ActuatableProperty ;
                 <http://schema.org/rangeIncludes> sosa:Actuation ;
                 rdfs:comment "Relation between an ActuatableProperty of a FeatureOfInterest and an Actuation changing its state."@en ;
                 rdfs:isDefinedBy sosa: ;
                 rdfs:label "is acted on by"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Relation between an ActuatableProperty of a FeatureOfInterest and an Actuation changing its state."@en ;
                 <http://www.w3.org/2004/02/skos/core#example> "In the activity (Actuation) of automatically closing a window if the temperature in a room drops below 20 degrees Celsius, the property on which the Actuator acts upon is the state of the window as it changes from being open to being closed. "@en .


###  http://www.w3.org/ns/sosa/isFeatureOfInterestOf
sosa:isFeatureOfInterestOf rdf:type owl:ObjectProperty ;
                           <http://schema.org/domainIncludes> sosa:FeatureOfInterest ,
                                                              sosa:Sample ;
                           <http://schema.org/rangeIncludes> sosa:Actuation ,
                                                             sosa:Observation ,
                                                             sosa:Sampling ;
                           rdfs:comment "A relation between a FeatureOfInterest and an Observation about it, an Actuation acting on it, or an act of Sampling that sampled it."@en ;
                           rdfs:isDefinedBy sosa: ;
                           rdfs:label "is feature of interest of"@en ;
                           <http://www.w3.org/2004/02/skos/core#definition> "A relation between a FeatureOfInterest and an Observation about it, an Actuation acting on it, or an act of Sampling that sampled it."@en .


###  http://www.w3.org/ns/sosa/isHostedBy
sosa:isHostedBy rdf:type owl:ObjectProperty ;
                <http://schema.org/domainIncludes> sosa:Actuator ,
                                                   sosa:Platform ,
                                                   sosa:Sampler ,
                                                   sosa:Sensor ;
                <http://schema.org/rangeIncludes> sosa:Platform ;
                rdfs:comment "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en ;
                rdfs:isDefinedBy sosa: ;
                rdfs:label "is hosted by"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en .


###  http://www.w3.org/ns/sosa/isObservedBy
sosa:isObservedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf sosa:observes ;
                  <http://schema.org/domainIncludes> sosa:ObservableProperty ;
                  <http://schema.org/rangeIncludes> sosa:Sensor ;
                  rdfs:comment "Relation between an ObservableProperty and the Sensor able to observe it."@en ;
                  rdfs:isDefinedBy sosa: ;
                  rdfs:label "is observed by"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Relation between an ObservableProperty and the Sensor able to observe it."@en .


###  http://www.w3.org/ns/sosa/isResultOf
sosa:isResultOf rdf:type owl:ObjectProperty ;
                <http://schema.org/domainIncludes> sosa:Result ,
                                                   sosa:Sample ;
                <http://schema.org/rangeIncludes> sosa:Actuation ,
                                                  sosa:Observation ,
                                                  sosa:Sampling ;
                rdfs:comment "Relation linking a Result to the Observation or Actuation or act of Sampling that created or caused it."@en ;
                rdfs:isDefinedBy sosa: ;
                rdfs:label "is result of"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Relation linking a Result to the Observation or Actuation or act of Sampling that created or caused it."@en .


###  http://www.w3.org/ns/sosa/isSampleOf
sosa:isSampleOf rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                <http://schema.org/domainIncludes> sosa:Sample ;
                <http://schema.org/rangeIncludes> sosa:FeatureOfInterest ;
                rdfs:comment "Relation from a Sample to the FeatureOfInterest that it is intended to be representative of."@en ;
                rdfs:isDefinedBy sosa: ;
                rdfs:label "is sample of"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Relation from a Sample to the FeatureOfInterest that it is intended to be representative of."@en .


###  http://www.w3.org/ns/sosa/madeActuation
sosa:madeActuation rdf:type owl:ObjectProperty ;
                   owl:inverseOf sosa:madeByActuator ;
                   <http://schema.org/domainIncludes> sosa:Actuator ;
                   <http://schema.org/rangeIncludes> sosa:Actuation ;
                   rdfs:comment "Relation between an Actuator and the Actuation it has made."@en ;
                   rdfs:isDefinedBy sosa: ;
                   rdfs:label "made actuation"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "Relation between an Actuator and the Actuation it has made."@en .


###  http://www.w3.org/ns/sosa/madeByActuator
sosa:madeByActuator rdf:type owl:ObjectProperty ;
                    <http://schema.org/domainIncludes> sosa:Actuation ;
                    <http://schema.org/rangeIncludes> sosa:Actuator ;
                    rdfs:comment "Relation linking an Actuation to the Actuator that made that Actuation."@en ;
                    rdfs:isDefinedBy sosa: ;
                    rdfs:label "made by actuator"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Relation linking an Actuation to the Actuator that made that Actuation."@en .


###  http://www.w3.org/ns/sosa/madeBySampler
sosa:madeBySampler rdf:type owl:ObjectProperty ;
                   owl:inverseOf sosa:madeSampling ;
                   <http://schema.org/domainIncludes> sosa:Sampling ;
                   <http://schema.org/rangeIncludes> sosa:Sampler ;
                   rdfs:comment "Relation linking an act of Sampling to the Sampler (sampling device or entity) that made it."@en ;
                   rdfs:isDefinedBy sosa: ;
                   rdfs:label "made by sampler"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "Relation linking an act of Sampling to the Sampler (sampling device or entity) that made it."@en .


###  http://www.w3.org/ns/sosa/madeBySensor
sosa:madeBySensor rdf:type owl:ObjectProperty ;
                  owl:inverseOf sosa:madeObservation ;
                  <http://schema.org/domainIncludes> sosa:Observation ;
                  <http://schema.org/rangeIncludes> sosa:Sensor ;
                  rdfs:comment "Relation between an Observation and the Sensor which made the Observation."@en ;
                  rdfs:isDefinedBy sosa: ;
                  rdfs:label "made by sensor"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Relation between an Observation and the Sensor which made the Observation."@en .


###  http://www.w3.org/ns/sosa/madeObservation
sosa:madeObservation rdf:type owl:ObjectProperty ;
                     <http://schema.org/domainIncludes> sosa:Sensor ;
                     <http://schema.org/rangeIncludes> sosa:Observation ;
                     rdfs:comment "Relation between a Sensor and an Observation made by the Sensor."@en ;
                     rdfs:isDefinedBy sosa: ;
                     rdfs:label "made observation"@en ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Sensor and an Observation made by the Sensor."@en .


###  http://www.w3.org/ns/sosa/madeSampling
sosa:madeSampling rdf:type owl:ObjectProperty ;
                  <http://schema.org/domainIncludes> sosa:Sampler ;
                  <http://schema.org/rangeIncludes> sosa:Sampling ;
                  rdfs:comment "Relation between a Sampler (sampling device or entity) and the Sampling act it performed."@en ;
                  rdfs:isDefinedBy sosa: ;
                  rdfs:label "made sampling"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Sampler (sampling device or entity) and the Sampling act it performed."@en .


###  http://www.w3.org/ns/sosa/observedProperty
sosa:observedProperty rdf:type owl:ObjectProperty ;
                      <http://schema.org/domainIncludes> sosa:Observation ;
                      <http://schema.org/rangeIncludes> sosa:ObservableProperty ;
                      rdfs:comment "Relation linking an Observation to the property that was observed. The ObservableProperty should be a property of the FeatureOfInterest (linked by hasFeatureOfInterest) of this Observation."@en ;
                      rdfs:isDefinedBy sosa: ;
                      rdfs:label "observed property"@en ;
                      <http://www.w3.org/2004/02/skos/core#definition> "Relation linking an Observation to the property that was observed. The ObservableProperty should be a property of the FeatureOfInterest (linked by hasFeatureOfInterest) of this Observation."@en .


###  http://www.w3.org/ns/sosa/observes
sosa:observes rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ssn:forProperty .


###  http://www.w3.org/ns/sosa/phenomenonTime
sosa:phenomenonTime rdf:type owl:ObjectProperty ;
                    <http://schema.org/domainIncludes> sosa:Actuation ,
                                                       sosa:Observation ,
                                                       sosa:Sampling ;
                    rdfs:comment "The time that the Result of an Observation, Actuation or Sampling applies to the FeatureOfInterest. Not necessarily the same as the resultTime. May be an Interval or an Instant, or some other compound TemporalEntity."@en ;
                    rdfs:isDefinedBy sosa: ;
                    rdfs:label "phenomenon time"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "The time that the Result of an Observation, Actuation or Sampling applies to the FeatureOfInterest. Not necessarily the same as the resultTime. May be an Interval or an Instant, or some other compound TemporalEntity."@en .


###  http://www.w3.org/ns/sosa/usedProcedure
sosa:usedProcedure rdf:type owl:ObjectProperty ;
                   owl:propertyChainAxiom ( sosa:madeByActuator
                                            ssn:implements
                                          ) ,
                                          ( sosa:madeBySampler
                                            ssn:implements
                                          ) ,
                                          ( sosa:madeBySensor
                                            ssn:implements
                                          ) ;
                   <http://schema.org/domainIncludes> sosa:Actuation ,
                                                      sosa:Observation ,
                                                      sosa:Sampling ;
                   <http://schema.org/rangeIncludes> sosa:Procedure ;
                   rdfs:comment "A relation to link to a re-usable Procedure used in making an Observation, an Actuation, or a Sample, typically through a Sensor, Actuator or Sampler."@en ;
                   rdfs:isDefinedBy sosa: ;
                   rdfs:label "used procedure"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "A relation to link to a re-usable Procedure used in making an Observation, an Actuation, or a Sample, typically through a Sensor, Actuator or Sampler."@en .


###  http://www.w3.org/ns/ssn/deployedOnPlatform
ssn:deployedOnPlatform rdf:type owl:ObjectProperty ;
                       owl:inverseOf ssn:inDeployment ;
                       rdfs:comment "Relation between a Deployment and the Platform on which the Systems are deployed."@en ;
                       rdfs:isDefinedBy ssn: ;
                       rdfs:label "deployed on platform"@en ;
                       <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Deployment and the Platform on which the Systems are deployed."@en .


###  http://www.w3.org/ns/ssn/deployedSystem
ssn:deployedSystem rdf:type owl:ObjectProperty ;
                   owl:inverseOf ssn:hasDeployment ;
                   rdfs:comment "Relation between a Deployment and a deployed System."@en ;
                   rdfs:isDefinedBy ssn: ;
                   rdfs:label "deployed system"@en ;
                   <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Deployment and a deployed System."@en .


###  http://www.w3.org/ns/ssn/detects
ssn:detects rdf:type owl:ObjectProperty ;
            rdfs:comment "A relation from a Sensor to the Stimulus that the Sensor can detect. The Stimulus itself will be serving as a proxy for some ObservableProperty."@en ,
                         "A relation from a Sensor to the Stimulus that the Sensor detects. The Stimulus itself will be serving as a proxy for some ObservableProperty."@en ;
            rdfs:isDefinedBy ssn: ;
            rdfs:label "detects"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> "A relation from a Sensor to the Stimulus that the Sensor detects. The Stimulus itself will be serving as a proxy for some ObservableProperty."@en .


###  http://www.w3.org/ns/ssn/forProperty
ssn:forProperty rdf:type owl:ObjectProperty ;
                rdfs:comment "A relation between some aspect of an entity and a Property."@en ;
                rdfs:isDefinedBy ssn: ;
                rdfs:label "for property"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "A relation between some aspect of an entity and a Property."@en ;
                <http://www.w3.org/2004/02/skos/core#example> "For example, from a Sensor to the properties it can observe; from an Actuator to the properties it can act on; from a Deployment to the properties it was installed to observe or act on; from a SystemCapability to the Property the capability is described for."@en .


###  http://www.w3.org/ns/ssn/hasDeployment
ssn:hasDeployment rdf:type owl:ObjectProperty ;
                  rdfs:comment "Relation between a System and a Deployment, recording that the System is deployed in that Deployment."@en ;
                  rdfs:isDefinedBy ssn: ;
                  rdfs:label "has deployment"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Relation between a System and a Deployment, recording that the System is deployed in that Deployment."@en .


###  http://www.w3.org/ns/ssn/hasInput
ssn:hasInput rdf:type owl:ObjectProperty ;
             rdfs:comment "Relation between a Procedure and an Input to it."@en ;
             rdfs:isDefinedBy ssn: ;
             rdfs:label "has input"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Procedure and an Input to it."@en .


###  http://www.w3.org/ns/ssn/hasOutput
ssn:hasOutput rdf:type owl:ObjectProperty ;
              rdfs:comment "Relation between a Procedure and an Output of it."@en ;
              rdfs:isDefinedBy ssn: ;
              rdfs:label "has output"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Procedure and an Output of it."@en .


###  http://www.w3.org/ns/ssn/hasProperty
ssn:hasProperty rdf:type owl:ObjectProperty ;
                owl:inverseOf ssn:isPropertyOf ;
                rdfs:comment "Relation between an entity and a Property of that entity."@en ;
                rdfs:isDefinedBy ssn: ;
                rdfs:label "has property"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "Relation between an entity and a Property of that entity."@en .


###  http://www.w3.org/ns/ssn/hasSubSystem
ssn:hasSubSystem rdf:type owl:ObjectProperty ;
                 rdfs:comment "Relation between a System and its component parts."@en ;
                 rdfs:isDefinedBy ssn: ;
                 rdfs:label "has subsystem"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Relation between a System and its component parts."@en .


###  http://www.w3.org/ns/ssn/implementedBy
ssn:implementedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf ssn:implements ;
                  rdfs:comment "Relation between a Procedure (an algorithm, procedure or method) and an entity that implements that Procedure in some executable way."@en ;
                  rdfs:isDefinedBy ssn: ;
                  rdfs:label "implemented by"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Procedure (an algorithm, procedure or method) and an entity that implements that Procedure in some executable way."@en ;
                  <http://www.w3.org/2004/02/skos/core#example> "For example, the relationship between a scientific measuring Procedure and a sensor that senses via that Procedure."@en .


###  http://www.w3.org/ns/ssn/implements
ssn:implements rdf:type owl:ObjectProperty ;
               rdfs:comment "Relation between an entity that implements a Procedure in some executable way and the Procedure (an algorithm, procedure or method)."@en ;
               rdfs:isDefinedBy ssn: ;
               rdfs:label "implements"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relation between an entity that implements a Procedure in some executable way and the Procedure (an algorithm, procedure or method)."@en ;
               <http://www.w3.org/2004/02/skos/core#example> "For example, the relationship between a sensor and the scientific measuring Procedure via which it senses."@en .


###  http://www.w3.org/ns/ssn/inDeployment
ssn:inDeployment rdf:type owl:ObjectProperty ;
                 rdfs:comment "Relation between a Platform and a Deployment, meaning that the deployedSystems of the Deployment are hosted on the Platform."@en ;
                 rdfs:isDefinedBy ssn: ;
                 rdfs:label "in deployment"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Platform and a Deployment, meaning that the deployedSystems of the Deployment are hosted on the Platform."@en ;
                 <http://www.w3.org/2004/02/skos/core#example> "For example, a relation between a buoy and a deployment of several Sensors."@en .


###  http://www.w3.org/ns/ssn/isPropertyOf
ssn:isPropertyOf rdf:type owl:ObjectProperty ;
                 rdfs:comment "Relation between a Property and the entity it belongs to."@en ;
                 rdfs:isDefinedBy ssn: ;
                 rdfs:label "is property of"@en ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Property and the entity it belongs to."@en .


###  http://www.w3.org/ns/ssn/isProxyFor
ssn:isProxyFor rdf:type owl:ObjectProperty ;
               rdfs:comment "A relation from a Stimulus to the Property that the Stimulus is serving as a proxy for."@en ;
               rdfs:isDefinedBy ssn: ;
               rdfs:label "is proxy for"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "A relation from a Stimulus to the Property that the Stimulus is serving as a proxy for."@en ;
               <http://www.w3.org/2004/02/skos/core#example> "For example, the expansion of quicksilver is a stimulus that serves as a proxy for some temperature property. An increase or decrease in the velocity of spinning cups on a wind sensor is serving as a proxy for the wind speed."@en .


###  http://www.w3.org/ns/ssn/wasOriginatedBy
ssn:wasOriginatedBy rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:comment "Relation between an Observation and the Stimulus that originated it."@en ;
                    rdfs:isDefinedBy ssn: ;
                    rdfs:label "was originated by"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "Relation between an Observation and the Stimulus that originated it."@en .


